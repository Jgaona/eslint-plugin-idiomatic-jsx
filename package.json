{
  "name": "eslint-plugin-idiomatic-jsx",
  "version": "1.0.0",
  "description": "Eslint jsx plugin to bring more idiomatic control over jsx",
  "main": "lib/index.js",
  "files": [
    "/lib"
  ],
  "repository": "https://github.com/danrigsby/eslint-plugin-idiomatic-jsx",
  "author": "Dan Rigsby <danrigsby@gmail.com>",
  "license": "MIT",
  "keywords": [
    "eslint-plugin",
    "eslint",
    "eslintplugin",
    "jsx"
  ],
  "scripts": {
    "start": "npm run lint:fix && rimraf lib && babel src --out-dir lib --copy-files --ignore __test__",
    "build": "npm run lint && rimraf lib && babel src --out-dir lib --copy-files --ignore __test__",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint src",
    "test": "mocha \"src/**/*.test.js\" --compilers js:babel-core/register",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "jsx-ast-utils": "^1.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "mocha": "^3.4.1",
    "rimraf": "^2.6.1"
  }
}
